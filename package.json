{
	"name": "pinit",
	"displayName": "PinIt",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"activationEvents_save": [
		"onView:pinItExplorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "pinItExplorer",
					"name": "Pin It"
				}
			]
		},
		"commands": [
			{
				"command": "pinList.addEntry",
				"title": "Pin This"
			},
			{
				"command": "pinList.removeEntry",
				"title": "Delete"
			},
			{
				"command": "pinList.clearAllEntry",
				"title": "Clear All"
			},
			{
				"command": "pinList.openEntry",
				"title": "Open"
			}
		],

		"menus": {
			"view/title": [
				{
					"command": "pinList.addEntry",
					"when": "view == pinItExplorer"
				}
			],
			"view/item/context": [
				{
					"command": "pinList.removeEntry",
					"when": "view == pinItExplorer && viewItem == $Pin",
					"group": "inline"
				},
				{
					"command": "pinList.clearAllEntry",
					"when": "view == pinItExplorer && viewItem == $Pin"
				},
				{
					"command": "pinList.removeEntry",
					"when": "view == pinItExplorer && viewItem == $Pin"
				},
				{
					"command": "pinList.openEntry",
					"when": "view == pinItExplorer && viewItem == $Pin"
				}
			],
			"editor/context": [{
				"command": "pinList.addEntry",
				"group": "PinIt"
			  }
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
